<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <style>
      th {
        margin: 0rem 1rem;
        width: 8rem;
        display: flex;
        justify-content: flex-start;
        font-size: 16px;
      }
      td {
        margin: 1rem 1rem;
        width: 8rem;
      }
      .sku {
        width: 12rem;
      }
      .qty {
        width: 3rem;
      }

    </style>  
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Results</h1>
    <div>
      <table style="width: 70vw">
        <tr style="display: flex; ">
          <th class="sku">WarehouseSku</th>
          <th>PalletId</th>
          <th>LocationName</th>
          <th class="qty">Qty</th>
          <th class="qty">QtyToTransfer</th>
        </tr>
        <% for(var i=0; i < result.length; i++) { %>
        <tr style="display: flex; ">
          <td class="sku"><%= result[i].WarehouseSku %></td>
          <td><%= result[i].LocationName %></td>
          <td><%= result[i].PalletId %></td>
          <td class="qty"><%= result[i].Qty %></td>
          <td class="qty"><%= result[i].qtyToTransfer %></td>
        </tr>
        <% } %>
      </table>
    </div>
  </body>
</html>
